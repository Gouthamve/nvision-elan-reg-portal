<div class="container">
  <h2>Team Registration Table</h2>
  <table class="table table-hover table-bordered">
    <thead>
      <tr>
        <th>IITH ID 1</th>
        <th>IITH ID 2</th>
        <th>IITH ID 3</th>
        <th>IITH ID 4</th>
        <th>IITH ID 5</th>
        <th>Event Name</th>
        <th>TeamID</th>
      </tr>
    </thead>
    <tbody>
      {{#each teams}}
        <tr>
          <td>{{this.id1}}</td>
          <td>{{this.id2}}</td>
          <td>{{this.id3}}</td>
          <td>{{this.id4}}</td>
          <td>{{this.id5}}</td>
          <td>{{this.event}}</td>
          <td>{{this.teamId}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
