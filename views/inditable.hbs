<div class="container">
  <h2>Individual Registration Table</h2>
  <table class="table table-hover table-bordered">
    <thead>
      <tr>
        <th>IITH ID</th>
        <th>Name</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>College</th>
        <th>Gender</th>
      </tr>
    </thead>
    <tbody>
      {{#each users}}
        <tr>
          <td>{{this.iithId}}</td>
          <td>{{this.name}}</td>
          <td>{{this.email}}</td>
          <td>{{this.phone}}</td>
          <td>{{this.college}}</td>
          <td>{{this.gender}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>